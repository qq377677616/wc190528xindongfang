/* pages/index_detail/index_detail.wxss */
.container {
   padding: 0;
}
.content{
   width: 100%;
   height: 100%;
   padding-bottom: 100rpx;
}
.banner{
   width: 100%;
}
.banner image{
   width: 100%;
}
.title{
   padding: 0 25rpx;
   font-size: 36rpx;
   /* font-weight: bold; */
}
.yubaoming{
   color: #959595;
   padding: 30rpx 25rpx;
   border-bottom: 1px solid #eeeeee;
   font-size: 32rpx;
   position: relative;
}
.yubaoming .moneny{
   color: #fe5959;
   font-size: 36rpx;
   /* font-weight: bold; */
}
.yubaoming .rmb{
   font-size: 24rpx;
}
.yubaoming .shares::after{
   border: none;
}
.yubaoming .shares{
   position: absolute;
   right: 40rpx;
   top: 10px;
   background:#fff;
}
.shares view{
   line-height:15px;
   font-size:24rpx;
   color: #787878;
}
.shares image{
   display: block;
   width: 36rpx;
   /* height: 60rpx; */
   margin: auto;
   padding-top: 10rpx;
   padding-bottom: 6rpx;
}
.info,.tuanqi,.liangdian,.userComment,.setoutDate{
   padding: 30rpx 25rpx;
   border-bottom: 1px solid #eeeeee;
}
.info{
   padding-bottom: 0;
   border: none;
}
.tuanqi{
   padding-top: 0;
}
.info1,.info2,.info3,.info4,.info5{
   font-size: 32rpx;
   color: #959595;
   padding-bottom: 20rpx;
}
.font{
   color: #1c1c1c;
}
.inline{
   display: inline-block;
   width: 75%;
   vertical-align: top;
}
.info5{
   padding-bottom: 27rpx;
   text-align: left;
}
.info5 div{
   padding: 6rpx 10rpx;
   border: 1px solid #cccccc;
   font-size: 26rpx;
   color: #cccccc;
   border-radius: 6rpx;
   margin-right: 12rpx;
}
.t1,.t2{
   font-size: 32rpx;
   color: #959595;
   padding-bottom: 22rpx;
   overflow: hidden;
}
